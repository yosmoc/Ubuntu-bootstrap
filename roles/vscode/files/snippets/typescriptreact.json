    // Array
    "map": {
        "prefix": "ma",
        "body": [
            "${1:[]}.map((${2:val}) => ${2:val})"
        ],
        "description": "map"
    },
    "find": {
        "prefix": "fin",
        "body": [
            "${1:[]}.find((${2:val}) => ${2:val} === true)"
        ],
        "description": "find"
    },
    "filter": {
        "prefix": "fil",
        "body": [
            "${1:[]}.filter((${2:val}) => ${2:val} === true)"
