    },
    "split": {
        "prefix": "sp",
        "body": [
            "${1:''}.split('${2:,}')"
        ],
        "description": "split"
    },
    "reduce": {
        "prefix": "red",
        "body": [
            "${1:[]}.reduce((sum, current) => sum + ${2:current}, 0)"
