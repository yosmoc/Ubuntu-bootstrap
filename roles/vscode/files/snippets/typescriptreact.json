        ],
        "description": "alert"
    },
    "debugger": {
        "prefix": "deb",
        "body": [
            "debugger"
        ],
        "description": "debugger"
    },
    // loop
    "for": {
        "prefix": "fr",
        "body": [
            "for (let ${1:i} = 0; ${1:i} < ${2:count}; ++${1:i}) {",
            "  ${3}",
            "}"
        ],
        "description": "for in"
    },
    "for in": {
        "prefix": "fi",
        "body": [
            "for (let key in ${1:obj}s) {",
            "  const ${1:obj} = ${1:obj}s[key]",
            "}"
        ],
        "description": "for in"
    },
    "for of": {
        "prefix": "fo",
        "body": [
            "for (let val of ${1:arr}) {",
            "  ",
            "}"
        ],
        "description": "for of"
    },
    "forEach": {
        "prefix": "fe",
        "body": [
            "Object.keys(${1:obj}).forEach(key => {",
            "  ",
            "})"
        ],
        "description": "forEach"
    },
    "Object keys": {
        "prefix": "ok",
        "body": [
            "Object.keys(${1:obj})"
        ],
        "description": "Object keys"
    },
    "Object values": {
        "prefix": "ov",
        "body": [
            "Object.values(${1:obj})"
