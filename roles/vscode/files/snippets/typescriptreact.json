        "prefix": "del",
        "body": [
            "delete ",
        ],
        "description": "delete"
    },
    "continue": {
        "prefix": "co",
        "body": [
            "continue",
        ],
        "description": "continue"
    },
    "break": {
        "prefix": "br",
        "body": [
            "break",
        ],
        "description": "break"
    },
    "switch": {
        "prefix": "sw",
        "body": [
            "switch (${1:val}) {",
            "  case ${2:match}:",
            "    break",
            "  default:",
