    "slice": {
        "prefix": "sl",
        "body": [
            "${1:[]}.slice(${2:0, 5})"
        ],
        "description": "slice"
    },
    "push": {
        "prefix": "pu",
        "body": [
            "${1:[]}.push(${2:val})"
        ],
        "description": "push"
